{
  "name": "graphql-js-client",
  "version": "0.0.1",
  "description": "Feature light client library for fetching resources via GraphQL",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "Shopify Inc.",
  "dependencies": {},
  "devDependencies": {
    "@shopify/graphql-js-schema": "0.0.2",
    "@shopify/graphql-js-schema-fetch": "0.0.1",
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-shopify-header": "1.0.6",
    "babel-plugin-eslint-test-generator": "1.0.2",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-preset-shopify": "15.0.1",
    "babel-register": "6.18.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.3",
    "eslint": "3.8.1",
    "eslint-plugin-shopify": "14.0.0",
    "expect.js": "0.3.1",
    "fb-watchman": "1.9.0",
    "http-server": "0.9.0",
    "jq-cli-wrapper": "0.3.0",
    "jquery": "3.1.1",
    "mocha": "3.1.2",
    "rollup": "0.36.3",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-commonjs": "5.0.5",
    "rollup-plugin-multi-entry": "2.0.1",
    "rollup-plugin-node-builtins": "2.0.0",
    "rollup-plugin-node-globals": "1.0.9",
    "rollup-plugin-node-resolve": "2.0.0",
    "wait-on": "1.5.3"
  },
  "scripts": {
    "start": "npm run clean && npm run setup-paths && concurrently 'npm run test:tdd-deps' 'npm run test:watch' 'http-server -p 4200 .tmp/test' 'npm run -s print-start-message'",
    "clean": "rimraf .tmp dist",
    "setup-paths": "mkdir -p dist .tmp/test",
    "test": "npm run lint && npm run mocha",
    "test:tdd-deps": "concurrently 'npm run test:copy-deps' 'npm run types-fixture:build'",
    "test:copy-deps": "cp test/index.html node_modules/mocha/mocha.* node_modules/jquery/dist/jquery.slim.min.js .tmp/test/ && cp node_modules/expect.js/index.js .tmp/test/expect.js",
    "test:watch": "cross-env BABEL_ENV='tdd' node scripts/test-build-watch.js",
    "test:build": "cross-env BABEL_ENV='tdd' node scripts/test-build-once.js",
    "prepublish": "babel -d lib/ src/",
    "mocha": "npm run types-fixture:build && cross-env BABEL_ENV='development' mocha --compilers js:babel-register -u tdd $(npm run -s mocha:reporter-args) test/**/*.js",
    "mocha:reporter-args": "[ -n \"${CI}\" ] && echo --reporter xunit --reporter-options output=$CIRCLE_TEST_REPORTS/junit/mocha.xml",
    "lint": "eslint --max-warnings 0 -c .eslintrc.json $(npm run -s lint:reporter-args) src/ test/",
    "lint:reporter-args": "[ -n \"${CI}\" ] && echo -o $CIRCLE_TEST_REPORTS/junit/eslint.xml -f junit",
    "print-start-message": "wait-on file:.tmp/test/index.html file:.tmp/test/tests.js tcp:4200 file:fixtures/types.js && echo \"\n\n⚡️⚡️⚡️  Good to go at http://localhost:4200 ⚡️⚡️⚡️\"",
    "types-fixture:refresh": "npm run types-fixture:fetch-graphql-schema && npm run types-fixture:build",
    "types-fixture:build": "rimraf fixtures/types.js && npm run types-fixture:transform-graphql-schema",
    "types-fixture:fetch-graphql-schema": "graphql-js-schema-fetch --url https://graphql.myshopify.com/api/graph --header 'Authorization: Basic MzUxYzEyMjAxN2QwZjJhOTU3ZDMyYWU3MjhhZDc0OWM=' | jq '.' > schema.json",
    "types-fixture:transform-graphql-schema": "graphql-js-schema --schema-file schema.json --outdir fixtures --schema-bundle-name 'Types' --bundle-only"
  },
  "keywords": [
    "graphql,api,client"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Shopify/graphql-js-client.git"
  },
  "homepage": "https://github.com/Shopify/graphql-js-client",
  "bugs": {
    "url": "https://github.com/Shopify/graphql-js-client/issues"
  }
}
